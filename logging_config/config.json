{

    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "[%(asctime)s]: %(message)s"
        }
    },
    "handlers": {
        "io_stream": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "simple"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO", 
            "handlers": ["io_stream"]}
        }
}
